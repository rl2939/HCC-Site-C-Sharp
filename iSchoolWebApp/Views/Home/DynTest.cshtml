@model dynamic
@{
    ViewData["Title"] = Model.pageTitle;
}

<h1>@Model.About.title</h1>
<section class="about">
    <p>@Model.About.description</p>
    <p>@Model.About.quote</p>
    <p>--- @Model.About.quoteAuthor</p>
</section>
<h1>Second Model - course</h1>
<section class="about">
    <p>@Model.Course.courseID</p>
    <p>@Model.Course.title</p>
    <p>@Model.Course.description</p>
</section>