@model iSchoolWebApp.Models.NewsModel
@{
    ViewData["Title"] = @Model.pageTitle;
}

<h1>News</h1>

<div id="allNews" style="display:none">
    <div id="NewsAcc">
        @foreach (var item in Model.older)
        {
            <h2>@item.title (@item.date)</h2>
            <div>
                <p>@item.description</p>
            </div>
        }
    </div>
</div>


